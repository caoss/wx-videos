/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
page {
  color: #666;
  font-size: 28rpx;
  background-color: #fff;
}
.themeTextColor {
  color: #5FC9AF
}
.tags {
  display: inline-block;
  line-height: 28rpx;
  border: 2rpx solid #5FC9AF;
  padding: 4rpx 8rpx 2rpx;
  margin-right: 16rpx;
  font-size: 20rpx;
  color: #5FC9AF;
  border-radius: 6rpx;
}
.tags_download {
  color: #FF8033;
  border-color: #FF8033;
}
.loading {
  position: absolute;
  width: 100%;
  top: 0;
  bottom: 0;
}
.loadingImg {
  position: relative;
  top: 50%;
  left: 50%;
  width: 64rpx;
  height: 64rpx;
  transform: translateX(-50%);
}
